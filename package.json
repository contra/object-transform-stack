    "eslint": "^7.0.0",
    "express": "^4.16.4",
    "mocha": "^8.0.0",
    "rimraf": "^3.0.0",
    "should": "^13.0.0"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^1.0.0",
    "@turf/turf": "^5.1.6",
    "aguid": "^2.0.0",
    "capitalize": "^2.0.0",
    "convert-units": "^2.3.4",
    "dot-prop": "^5.0.0",
    "is-date-like": "^1.0.0",
    "is-number": "^7.0.0",
    "is-plain-obj": "^2.0.0",
    "lodash": "^4.17.20",
    "moment-timezone": "^0.5.25",
